selector
    cond IsClean(floor)
    sequence
        cond IsHoldingCleaningTool(self)
        cond IsNear(self,floor)
        act Wipe(floor)
    sequence
        cond IsHoldingCleaningTool(self)
        cond IsStanding(self)
        act Walk(floor)
    sequence
        cond IsRightHandEmpty(self)
        cond IsNear(self,rag)
        cond IsStanding(self)
        act RightGrab(rag)
    sequence
        cond IsRightHandEmpty(self)
        cond IsStanding(self)
        act Walk(rag)
