selector
    cond IsPlugged(fridge)
    sequence
        cond IsNear(self,fridge)
        cond IsUnplugged(fridge)
        cond IsLeftHandEmpty(self)
        act PlugIn(fridge)
    sequence
        cond IsUnplugged(fridge)
        cond IsStanding(self)
        cond IsLeftHandEmpty(self)
        act Walk(fridge)
