selector
    cond IsSwitchedOn(tv)
    sequence
        cond IsNear(self,tv)
        cond IsSwitchedOff(tv)
        cond IsLeftHandEmpty(self)
        cond IsPlugged(tv)
        act SwitchOn(tv)
    sequence
        cond IsNear(self,tv)
        cond IsSwitchedOff(tv)
        cond IsLeftHandEmpty(self)
        cond IsUnplugged(tv)
        act PlugIn(tv)
    sequence
        cond IsPlugged(tv)
        cond IsSwitchedOff(tv)
        cond IsLeftHandEmpty(self)
        cond IsStanding(self)
        act Walk(tv)
    sequence
        cond IsSwitchedOff(tv)
        cond IsLeftHandEmpty(self)
        cond IsStanding(self)
        cond IsUnplugged(tv)
        act Walk(tv)
