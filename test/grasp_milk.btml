selector
    cond IsIn(milk,fridge)
    sequence
        cond IsRightHolding(self,milk)
        cond IsOpen(fridge)
        cond IsNear(self,fridge)
        act RightPutIn(milk,fridge)
    sequence
        cond IsClose(fridge)
        cond IsRightHolding(self,milk)
        cond IsNear(self,fridge)
        cond IsLeftHandEmpty(self)
        act Open(fridge)
    sequence
        cond IsRightHolding(self,milk)
        cond IsOpen(fridge)
        cond IsStanding(self)
        act Walk(fridge)
    sequence
        cond IsClose(fridge)
        cond IsRightHolding(self,milk)
        cond IsStanding(self)
        cond IsLeftHandEmpty(self)
        act Walk(fridge)
    sequence
        cond IsNear(self,milk)
        cond IsOpen(fridge)
        cond IsStanding(self)
        cond IsRightHandEmpty(self)
        act RightGrab(milk)
    sequence
        cond IsRightHandEmpty(self)
        cond IsNear(self,milk)
        cond IsStanding(self)
        cond IsLeftHandEmpty(self)
        cond IsClose(fridge)
        act RightGrab(milk)
    sequence
        cond IsOpen(fridge)
        cond IsStanding(self)
        cond IsRightHandEmpty(self)
        act Walk(milk)
    sequence
        cond IsRightHandEmpty(self)
        cond IsClose(fridge)
        cond IsStanding(self)
        cond IsLeftHandEmpty(self)
        act Walk(milk)
