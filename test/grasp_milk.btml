selector
    cond IsClean(apple)
    sequence
        cond IsNear(self,faucet)
        cond IsRightHolding(self,apple)
        cond IsSwitchedOn(faucet)
        act Wash(apple)
    sequence
        cond IsStanding(self)
        cond IsRightHolding(self,apple)
        cond IsSwitchedOn(faucet)
        act Walk(faucet)
    sequence
        cond IsLeftHandEmpty(self)
        cond IsNear(self,faucet)
        cond IsStanding(self)
        cond IsSwitchedOff(faucet)
        cond IsRightHolding(self,apple)
        act SwitchOn(faucet)
    sequence
        cond IsNear(self,apple)
        cond IsStanding(self)
        cond IsRightHandEmpty(self)
        cond IsSwitchedOn(faucet)
        act RightGrab(apple)
    sequence
        cond IsStanding(self)
        cond IsRightHandEmpty(self)
        cond IsSwitchedOn(faucet)
        act Walk(apple)
    sequence
        cond IsLeftHandEmpty(self)
        cond IsNear(self,faucet)
        cond IsStanding(self)
        cond IsRightHandEmpty(self)
        cond IsSwitchedOff(faucet)
        act SwitchOn(faucet)
    sequence
        cond IsSwitchedOff(faucet)
        cond IsNear(self,faucet)
        cond IsRightHolding(self,apple)
        cond IsLeftHandEmpty(self)
        act SwitchOn(faucet)
    sequence
        cond IsSwitchedOff(faucet)
        cond IsStanding(self)
        cond IsRightHolding(self,apple)
        cond IsLeftHandEmpty(self)
        act Walk(faucet)
    sequence
        cond IsLeftHandEmpty(self)
        cond IsStanding(self)
        cond IsRightHandEmpty(self)
        cond IsNear(self,apple)
        cond IsSwitchedOff(faucet)
        act RightGrab(apple)
    sequence
        cond IsSwitchedOff(faucet)
        cond IsStanding(self)
        cond IsRightHandEmpty(self)
        cond IsLeftHandEmpty(self)
        act Walk(apple)
