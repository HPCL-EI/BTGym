selector
    cond IsSwitchedOn(tv)
    sequence
        cond IsLeftHandEmpty(self)
        cond IsSwitchedOff(tv)
        cond IsPlugged(tv)
        cond IsNear(self,tv)
        act SwitchOn(tv)
    sequence
        cond IsUnplugged(tv)
        cond IsSwitchedOff(tv)
        cond IsLeftHandEmpty(self)
        cond IsNear(self,tv)
        act PlugIn(tv)
    sequence
        cond IsSwitchedOff(tv)
        cond IsLeftHandEmpty(self)
        cond IsStanding(self)
        cond IsPlugged(tv)
        act Walk(tv)
    sequence
        cond IsSwitchedOff(tv)
        cond IsLeftHandEmpty(self)
        cond IsStanding(self)
        cond IsUnplugged(tv)
        act Walk(tv)
