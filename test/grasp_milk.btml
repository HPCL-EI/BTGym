selector
    sequence
        cond IsOn(pillow,bed)
        cond IsOn(coffeepot,coffeetable)
        cond IsOn(towel,towelrack)
    sequence
        cond IsOn(pillow,bed)
        cond IsNear(self,towelrack)
        cond IsRightHolding(self,towel)
        cond IsOn(coffeepot,coffeetable)
        act RightPut(towel,towelrack)
    sequence
        cond IsOn(pillow,bed)
        cond IsRightHolding(self,towel)
        cond IsOn(coffeepot,coffeetable)
        cond IsStanding(self)
        act Walk(towelrack)
    sequence
        cond IsNear(self,bed)
        cond IsRightHolding(self,pillow)
        cond IsRightHolding(self,towel)
        cond IsOn(coffeepot,coffeetable)
        cond IsStanding(self)
        act RightPut(pillow,bed)
    sequence
        cond IsRightHolding(self,towel)
        cond IsOn(coffeepot,coffeetable)
        cond IsStanding(self)
        cond IsRightHolding(self,pillow)
        act Walk(bed)
    sequence
        cond IsRightHolding(self,coffeepot)
        cond IsRightHolding(self,pillow)
        cond IsRightHolding(self,towel)
        cond IsStanding(self)
        cond IsNear(self,coffeetable)
        act RightPut(coffeepot,coffeetable)
    sequence
        cond IsRightHolding(self,coffeepot)
        cond IsRightHolding(self,towel)
        cond IsStanding(self)
        cond IsRightHolding(self,pillow)
        act Walk(coffeetable)
    sequence
        cond IsRightHolding(self,coffeepot)
        cond IsOn(pillow,bed)
        cond IsRightHolding(self,towel)
        cond IsStanding(self)
        cond IsNear(self,coffeetable)
        act RightPut(coffeepot,coffeetable)
    sequence
        cond IsOn(pillow,bed)
        cond IsRightHolding(self,coffeepot)
        cond IsRightHolding(self,towel)
        cond IsStanding(self)
        act Walk(coffeetable)
    sequence
        cond IsRightHandEmpty(self)
        cond IsOn(pillow,bed)
        cond IsNear(self,towel)
        cond IsOn(coffeepot,coffeetable)
        cond IsStanding(self)
        act RightGrab(towel)
    sequence
        cond IsOn(pillow,bed)
        cond IsOn(coffeepot,coffeetable)
        cond IsRightHandEmpty(self)
        cond IsStanding(self)
        act Walk(towel)
    sequence
        cond IsNear(self,bed)
        cond IsOn(coffeepot,coffeetable)
        cond IsStanding(self)
        cond IsRightHolding(self,pillow)
        act RightPut(pillow,bed)
    sequence
        cond IsOn(coffeepot,coffeetable)
        cond IsStanding(self)
        cond IsRightHolding(self,pillow)
        act Walk(bed)
    sequence
        cond IsRightHolding(self,coffeepot)
        cond IsNear(self,coffeetable)
        cond IsStanding(self)
        cond IsRightHolding(self,pillow)
        act RightPut(coffeepot,coffeetable)
    sequence
        cond IsRightHolding(self,coffeepot)
        cond IsStanding(self)
        cond IsRightHolding(self,pillow)
        act Walk(coffeetable)
    sequence
        cond IsNear(self,pillow)
        cond IsOn(coffeepot,coffeetable)
        cond IsRightHandEmpty(self)
        cond IsStanding(self)
        act RightGrab(pillow)
    sequence
        cond IsOn(coffeepot,coffeetable)
        cond IsRightHandEmpty(self)
        cond IsStanding(self)
        act Walk(pillow)
    sequence
        cond IsRightHolding(self,coffeepot)
        cond IsStanding(self)
        cond IsNear(self,coffeetable)
        act RightPut(coffeepot,coffeetable)
    sequence
        cond IsRightHolding(self,coffeepot)
        cond IsStanding(self)
        act Walk(coffeetable)
    sequence
        cond IsNear(self,coffeepot)
        cond IsRightHandEmpty(self)
        cond IsStanding(self)
        act RightGrab(coffeepot)
    sequence
        cond IsRightHandEmpty(self)
        cond IsStanding(self)
        act Walk(coffeepot)
