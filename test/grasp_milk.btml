selector
    cond IsSwitchedOn(tv)
    sequence
        cond IsSwitchedOff(tv)
        cond IsLeftHandEmpty(self)
        cond IsNear(self,tv)
        cond IsPlugged(tv)
        act SwitchOn(tv)
    sequence
        cond IsSwitchedOff(tv)
        cond IsLeftHandEmpty(self)
        cond IsNear(self,tv)
        cond IsUnplugged(tv)
        act PlugIn(tv)
    sequence
        cond IsSwitchedOff(tv)
        cond IsStanding(self)
        cond IsPlugged(tv)
        cond IsLeftHandEmpty(self)
        act Walk(tv)
    sequence
        cond IsSwitchedOff(tv)
        cond IsStanding(self)
        cond IsLeftHandEmpty(self)
        cond IsUnplugged(tv)
        act Walk(tv)
